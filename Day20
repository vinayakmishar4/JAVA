// inhertanice in intefacses
class Day20
{
  public static void main(String[] args) 
  {
  	 SampleClasss samples = new SampleClasss();
  	 samples.method1();
  	 samples.method2();
  	 samples.method3();
  	 samples.method4();
  }
}
interface sampleinterface
{
	void method1();
	void method2();
}
interface Childinterface extends sampleinterface
{
   void method3();
   void method4();
}
class SampleClasss implements Childinterface
{
	public void method1()
	{
		System.out.println("I am in method1 function");
	}
	public void method2()
	{
		System.out.println("I am in method2 function");
	}
	public void method3()
	{
		System.out.println("I am in method3 function");
	}
	public void method4()
	{
		System.out.println("I am in method4 function");
	}
}
