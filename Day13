 import java.util.*;
 class Day13
 // inhertanice in java
 {
  public static void main(String[] args)
  { 
  	Base B=new Base();
  	B.setX(4);
  	System.out.println(B.getX());
  } 
 }
 // Super Class
 class Base
 {
 	int x;
 	public int getX()
 	{
 		return x;
 	}
 	public void setX(int x)
 	{
 		this.x = x;
 	}
 	void printMe()
 	{
 		System.out.println("I am a Constructor");
 	}
 }
 class Dervied extends Base
 {
 	int y;
 	public int getY()
 	{
 		return y;
 	}
 	public void setY(int y)
 	{
 		this.y=y;
 	}
 }
